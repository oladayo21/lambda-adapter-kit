{
  "name": "@foladayo/lambda-adapter-kit",
  "version": "1.0.3-0",
  "description": "Framework-agnostic Lambda event and web request/response conversion utilities",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix",
    "format": "biome format . --write",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "aws",
    "lambda",
    "event-conversion",
    "web-api",
    "serverless",
    "converter",
    "utilities",
    "framework-agnostic"
  ],
  "author": "Oladayo Fagbemi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oladayo21/lambda-adapter-kit.git"
  },
  "homepage": "https://github.com/oladayo21/lambda-adapter-kit#readme",
  "bugs": {
    "url": "https://github.com/oladayo21/lambda-adapter-kit/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/aws-lambda": "^8.10.150",
    "@types/node": "^24.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
