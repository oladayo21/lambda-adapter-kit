{
  "name": "lambda-adapter-kit",
  "version": "1.0.0",
  "description": "Comprehensive toolkit for AWS Lambda adapters and web request/response conversion",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./handler": {
      "import": "./dist/handler.js",
      "types": "./dist/handler.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./converter": {
      "import": "./dist/converter.js",
      "types": "./dist/converter.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix",
    "format": "biome format . --write",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "aws",
    "lambda",
    "adapter",
    "serverless",
    "sveltekit",
    "event-conversion",
    "web-api",
    "toolkit"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/aws-lambda": "^8.10.150",
    "@types/node": "^24.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0"
  }
}
